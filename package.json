{
  "name": "avatarfi",
  "version": "0.0.9",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:export": "next build && next export",
    "start": "next start -p 8080",
    "prepare": "husky install",
    "release": "standard-version",
    "pre-commit": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "watch": "tsc -w -p ./tsconfig.json"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run prettier:fix"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.16.1",
    "@ant-design/icons": "^5.1.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@sendgrid/mail": "^7.7.0",
    "antd": "^5.0.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.5.3",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-redux": "^8.0.5",
    "react-select": "^5.7.0",
    "react-windowed-select": "^5.1.0",
    "redux-persist": "^6.0.0",
    "universal-cookie": "^6.0.0"
